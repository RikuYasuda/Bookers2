<div class="container">
  <div class="row">

   <div class="col-lg-4">

<%= render 'layouts/list', user: @user %>
<%= render 'layouts/new', book: @book %>

   </div>

   <div class="col-lg-8">

<% @users.each do |user| %>

<div>
  <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image.jpg" %>
  <span><%= user.name %></span>
  <span><%= link_to "show" , user_path %></span>

</div>
<% end %>

</div>

  </div>
</div>
